{
    "name": "express-boilerplate",
    "version": "1.0.1",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "mocha --require test/setup.js",
        "dev": "nodemon src/server.js",
        "start": "node src/server.js",
        "predeploy": "npm audit",
        "deploy": "git push heroku master",
        "lint": "node_modules/eslint/bin/eslint.js --fix"
    },
    "keywords": [],
    "engines": {
        "node": "16.15.0"
    },
    "eslintConfig": {
        "extends": "airbnb-base",
        "env": {
            "es6": true,
            "browser": true
        },
        "rules": {
            "brace-style": [
                "error",
                "stroustrup"
            ],
            "comma-dangle": [
                "error",
                "never"
            ],
            "no-unused-vars": [
                "warn"
            ],
            "no-var": [
                "off"
            ],
            "one-var": [
                "off"
            ],
            "import/order": [
                "off"
            ],
            "camelcase": [
                "off"
            ],
            "consistent-return": [
                "off"
            ],
            "no-console": [
                "off"
            ],
            "prefer-const": [
                "warn"
            ],
            "no-shadow": [
                "warn"
            ],
            "no-restricted-syntax": [
                "warn"
            ],
            "no-use-before-define": [
                "warn"
            ],
            "linebreak-style": [
                "off"
            ]
        }
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^7.0.0",
        "express": "^4.17.1",
        "helmet": "^3.21.2",
        "jade": "^0.29.0",
        "jsonwebtoken": "^9.0.0",
        "knex": "^2.4.2",
        "morgan": "^1.9.1",
        "nodemailer": "^6.9.0",
        "path": "^0.12.7",
        "pg": "^7.12.1",
        "stripe": "^8.123.0",
        "xss": "^1.0.8"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "eslint": "^7.13.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.22.1",
        "mocha": "^10.2.0",
        "nodemon": "^2.0.6",
        "postgrator-cli": "^3.1.0",
        "supertest": "^4.0.2"
    }
}
